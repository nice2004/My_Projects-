package edu.westmont.cs030.maze;

/**
 * Four cardinal directions (north, east, south, and west) arranged in a clockwise order starting with north.
 */
public enum Direction {
  /**
   * North (up) direction
   */
  NORTH,
  /**
   * East (right) direction
   */
  EAST,
  /**
   * South (down) direction
   */
  SOUTH,
  /**
   * West (left) direction
   */
  WEST
}
